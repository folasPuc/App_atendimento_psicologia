<MenuAdmin>:
    FloatLayout:
        canvas:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "bg2.jpg"
        Label:
            id: email_usuario
            text: "[b]Usuario_teste[/b]"
            bold: True
            markup: True
            pos_hint: {"right": 0.9, "top":0.85}
            size_hint: 0.8, 0.2
        Label:
            id: telefone_usuario
            text: "[b]Usuario_teste[/b]"
            bold: True
            markup: True
            pos_hint: {"right": 0.9, "top":0.65}
            size_hint: 0.8, 0.2
        GridLayout:
            rows: 1
            pos_hint: {"right": 1, "top": 0.12}
            size_hint: 1, 0.12
            canvas:
                Color:
                    rgb: utils.get_color_from_hex("#00607E")
                Rectangle:
                    size: self.size
                    pos: self.pos
            ImageButton:
                source: "historico_psico.png"
                on_release: print("marcar_horarios")
            ImageButton:
                source: "calendario_psico.png"
                on_release: print("sessoes marcadas")
            ImageButton:
                source: "ficha_psico.png"
                on_release: app.carregar_dias()
            ImageButton:
                source: "ajustes_psico.png"
                on_release: print("ajustes_adm")